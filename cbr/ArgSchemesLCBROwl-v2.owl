      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasCaseFactors"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="LeafFactors"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
