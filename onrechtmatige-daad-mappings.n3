                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:reele_verhouding_moeite_kosten_en_gevaar ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ])
              ] ;
      owl:equivalentClass
