:Gevaarzetting
      rdf:type owl:Class ;
      rdfs:subClassOf :Overtreding_Betamelijkheid ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:hasValue tv:gevaarzetting ;
                            owl:onProperty b:onrechtmatigheidsgrond
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:aard_en_omvang_gevreesde_schade ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:aard_van_de_gedraging ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:bezwaarlijkheid_voorzorgmaatregelen ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:kelderluik_criterium ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:waarschijnlijkheid_dat_schade_zich_zal_voordoen ;
                            owl:onProperty b:zorgvuldigheidsnorm
                          ])
              ] ;
      skos:note "Juridische aspecten van gevaarzetting"^^xsd:string .

