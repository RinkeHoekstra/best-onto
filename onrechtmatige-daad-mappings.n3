:Inbreuk
      rdf:type owl:Class ;
      rdfs:subClassOf bm:Mapping ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue tv:inbreuk_op_een_recht ;
                owl:onProperty b:onrechtmatigheidsgrond
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:unionOf ([ rdf:type owl:Restriction ;
                            owl:onProperty b:action ;
