      rdfs:subClassOf :Inbreuk ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue tv:opzettelijke_inbreuk ;
                owl:onProperty b:onrechtmatigheidsgrond
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf (:Inbreuk [ rdf:type owl:Restriction ;
                            owl:hasValue lv:opzettelijk ;
                            owl:onProperty b:situation
                          ])
              ] .

:Map-overmacht
      rdf:type owl:Class ;
      rdfs:subClassOf :Situatie ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
