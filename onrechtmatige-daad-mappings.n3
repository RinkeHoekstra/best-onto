              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Class ;
                            owl:unionOf ([ rdf:type owl:Restriction ;
                                        owl:hasValue lv:gevaarlijk_handelen ;
                                        owl:onProperty b:action
                                      ] [ rdf:type owl:Restriction ;
                                        owl:hasValue lv:gevaarlijke_situatie ;
                                        owl:onProperty b:object
                                      ])
                          ] [ rdf:type owl:Class ;
                            owl:complementOf :Map-gevaarlijk-handelen
                          ] [ rdf:type owl:Class ;
                            owl:complementOf :Map-gevaarzetting-algemeen-dader-orientatie
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue lv:lichamelijk_letsel ;
                            owl:onProperty b:result
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue lv:per_ongeluk ;
                            owl:onProperty b:situation
                          ])
              ] .

