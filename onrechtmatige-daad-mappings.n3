:Map-overtreding
      rdf:type owl:Class ;
      rdfs:subClassOf :Overtreding_Wet ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:hasValue tv:doen_of_nalaten_in_strijd_met_een_wettelijke_plicht ;
                            owl:onProperty b:onrechtmatigheidsgrond
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:omgekeerde_bewijslast_op_grond_van_wetsovertreding ;
                            owl:onProperty b:onrechtmatigheidsgrond
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:vervallen_vaststellen_rekenschap_belangen_van_benadeelde ;
                            owl:onProperty b:onrechtmatigheidsgrond
                          ])
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:unionOf ([ rdf:type owl:Restriction ;
                            owl:onProperty b:situation ;
                            owl:someValuesFrom
                                    [ rdf:type owl:Restriction ;
                                      owl:hasValue lv:overtreding ;
                                      owl:onProperty skos:broaderTransitive
                                    ]
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue lv:overtreding ;
                            owl:onProperty b:situation
                          ])
              ] ;
      skos:note "Bij het handelen in sterijd met een plicht is automatisch sprake van onrechtmatigheid. Voorbeelden: handelen zonder vergunning, het begaan van een verkeersovertreding, etc."@nl ;
      skos:prefLabel "Overtreding van een wettelijk voorschrift"^^xsd:string .

:Map-overtreding-beide-partijen
      rdf:type owl:Class ;
      rdfs:subClassOf :Overtreding_Wet ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue tv:wetsovertreding_door_eiser ;
                owl:onProperty b:onrechtmatigheidsgrond
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Class ;
                            owl:unionOf ([ rdf:type owl:Restriction ;
                                        owl:onProperty b:situation ;
                                        owl:someValuesFrom
                                                [ rdf:type owl:Restriction ;
                                                  owl:hasValue lv:overtreding ;
                                                  owl:onProperty skos:broaderTransitive
                                                ]
                                      ] [ rdf:type owl:Restriction ;
                                        owl:hasValue lv:overtreding ;
                                        owl:onProperty b:situation
                                      ])
                          ] [ rdf:type owl:Restriction ;
                            owl:minQualifiedCardinality
                                    "2"^^xsd:nonNegativeInteger ;
                            owl:onClass b:Concept ;
                            owl:onProperty b:actor
                          ])
              ] ;
      skos:note "Indien beide partijen een overtreding hebben begaan wordt de overtreding door de gedaagde minder zwaar gewogen"@nl ;
      skos:prefLabel "Overtreding door beide partijen"^^xsd:string .

:Map-samenloop-van-omstandigheden
