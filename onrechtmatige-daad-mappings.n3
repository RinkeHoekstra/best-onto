                                      owl:onProperty skos:broaderTransitive
                                    ]
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue lv:overmacht ;
                            owl:onProperty b:situation
                          ])
              ] ;
      skos:note "Als sprake is van overmacht kan dit als rechtvaardigheidsgrond gelden voor de daad. Hierdoor wordt hij in het algemeen niet als onrechtmatig aangemerkt"^^xsd:string ;
      skos:prefLabel "Overmacht als rechtvaardigingsgrond"^^xsd:string .

