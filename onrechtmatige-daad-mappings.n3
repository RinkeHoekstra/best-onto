:Map-overtreding
      rdf:type owl:Class ;
      rdfs:subClassOf :Overtreding ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:hasValue tv:doen_of_nalaten_in_strijd_met_een_wettelijke_plicht ;
                            owl:onProperty b:onrechtmatigheidsgrond
