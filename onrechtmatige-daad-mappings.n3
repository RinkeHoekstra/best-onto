:Map-inbreuk-auteursrecht
      rdf:type owl:Class ;
      rdfs:subClassOf :Inbreuk ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:hasValue tv:inbreuk_auteursrecht ;
                            owl:onProperty b:onrechtmatigheidsgrond
                          ] [ rdf:type owl:Restriction ;
                            owl:hasValue tv:toerekening_op_basis_van_schuld ;
                            owl:onProperty b:grond_toerekening_aan_dader
                          ])
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Class ;
                            owl:unionOf ([ rdf:type owl:Restriction ;
                                        owl:hasValue lv:kopieren ;
                                        owl:onProperty b:action
                                      ] [ rdf:type owl:Restriction ;
                                        owl:onProperty b:action ;
                                        owl:someValuesFrom
                                                [ rdf:type owl:Restriction ;
                                                  owl:hasValue lv:kopieren ;
                                                  owl:onProperty skos:broaderTransitive
                                                ]
                                      ])
                          ] [ rdf:type owl:Class ;
                            owl:unionOf ([ rdf:type owl:Restriction ;
                                        owl:hasValue lv:product ;
                                        owl:onProperty b:object
                                      ] [ rdf:type owl:Restriction ;
                                        owl:onProperty b:object ;
                                        owl:someValuesFrom
                                                [ rdf:type owl:Restriction ;
                                                  owl:hasValue lv:product ;
                                                  owl:onProperty skos:broaderTransitive
                                                ]
                                      ])
                          ])
              ] ;
      skos:changeNote "Mapping created on 20100604-151227"@en ;
      skos:definition """This mapping specifies that any case that has a description that matches all of the following layman terms: 

      best:action has value http://www.best-project.nl/owl/laymen-vocabulary.owl#kopieren (or narrower)
      best:object has value http://www.best-project.nl/owl/laymen-vocabulary.owl#product (or narrower)

      can be qualified as a case with the following legal description:

      best:onrechtmatigheidsgrond has value http://www.best-project.nl/owl/tort-vocabulary-new.owl#inbreuk_auteursrecht
      best:grond_toerekening_aan_dader has value http://www.best-project.nl/owl/tort-vocabulary-new.owl#toerekening_op_basis_van_schuld
      """@en ;
      skos:prefLabel "Inbreuk op een auteursrecht"@nl ;
      skos:scopeNote "Bij het kopieren van een product kan er sprake zijn van inbreuk op auteursrecht. Toerekening van de schade-veroorzakende daad is dan op basis van schuld: de dader is direct verantwoordelijk voor de handeling die tot de schade schade leidde."@nl .

