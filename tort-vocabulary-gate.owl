  <best:Concept rdf:ID="kans_dat_anderen_niet_goed_opletten">
    <rdfs:label xml:lang="nl">kans_dat_anderen_niet_goed_opletten</rdfs:label>
    <skos:prefLabel xml:lang="nl">kans dat anderen niet goed opletten</skos:prefLabel>
