  <rdf:Description rdf:about="http://www.best-project.nl/owl/tort-vocabulary.owl#onbehoorlijk_aanhaken_producten_of_diensten">
    <rdfs:label xml:lang="nl">reclame</rdfs:label>
    <rdfs:label xml:lang="nl">diensten</rdfs:label>
    <rdfs:label xml:lang="nl">handelsnaam</rdfs:label>
    <rdfs:label xml:lang="nl">vergelijking</rdfs:label>
    <rdfs:label xml:lang="nl">merk</rdfs:label>
