    <rdf:type rdf:resource="http://www.best-project.nl/owl/best.owl#Concept"/>
    <rdfs:label xml:lang="nl">gebruiksaanwijzing</rdfs:label>
    <rdfs:label xml:lang="nl">ernst</rdfs:label>
