  <rdf:Description rdf:about="http://www.best-project.nl/owl/tort-vocabulary.owl#kans_dat_daardoor_ongevallen_ontstaan">
    <rdf:type rdf:resource="http://www.best-project.nl/owl/best.owl#Concept"/>
    <rdfs:label xml:lang="nl">kans dat daardoor ongevallen ontstaan</rdfs:label>
