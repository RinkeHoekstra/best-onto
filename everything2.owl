    <to:fingerprint rdf:nodeID="A6590"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="nl">werkzaamheden_met_een_incidenteel_karakter</rdfs:label>
