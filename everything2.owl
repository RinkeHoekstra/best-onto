  <rdf:Description rdf:about="http://www.best-project.nl/owl/tort-vocabulary.owl#handelen_in_strijd_met_hetgeen_in_het_maatschappelijk_verkeer_betaamt">
    <rdfs:label xml:lang="nl">staking</rdfs:label>
    <rdfs:label xml:lang="nl">maatschappelijk verkeer</rdfs:label>
    <rdfs:label xml:lang="nl">hinder</rdfs:label>
    <rdfs:label xml:lang="nl">milieu</rdfs:label>
    <rdfs:label xml:lang="nl">leiding</rdfs:label>
    <rdfs:label xml:lang="nl">heersende opvatting</rdfs:label>
    <skos:altLabel>zorgvuldigheidsnorm</skos:altLabel>
    <rdfs:label xml:lang="nl">spel</rdfs:label>
    <rdfs:label xml:lang="nl">handelen in strijd met hetgeen in het maatschappelijk verkeer betaamt</rdfs:label>
    <rdfs:label xml:lang="nl">sport - en spel</rdfs:label>
    <skos:altLabel>maatschappelijke zorgvuldigheid</skos:altLabel>
    <rdfs:label xml:lang="nl">maatschappelijke zorgvuldigheid</rdfs:label>
    <rdfs:label xml:lang="nl">maatschappelijke betamelijkheid</rdfs:label>
    <rdfs:label xml:lang="nl">wanprestatie</rdfs:label>
    <skos:prefLabel xml:lang="nl">handelen in strijd met hetgeen in het maatschappelijk verkeer betaamt</skos:prefLabel>
    <rdfs:label xml:lang="nl">overheidsaansprakelijkheid</rdfs:label>
    <rdfs:label xml:lang="nl">ongelukkige samenloop van omstandigheden</rdfs:label>
    <rdfs:label xml:lang="nl">bestuurdersaansprakelijkheid</rdfs:label>
    <rdfs:label xml:lang="nl">verkeer</rdfs:label>
