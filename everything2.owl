    <skos:altLabel>zorgvuldigheidsnorm</skos:altLabel>
    <rdfs:label xml:lang="nl">waarschuwing</rdfs:label>
    <rdfs:label xml:lang="nl">belangenafweging</rdfs:label>
    <rdfs:label xml:lang="nl">kans dat anderen niet goed opletten</rdfs:label>
    <rdfs:label xml:lang="nl">onzorgvuldigheid</rdfs:label>
    <rdfs:label xml:lang="nl">in redelijkheid</rdfs:label>
