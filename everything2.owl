    <rdfs:label xml:lang="nl">na sluiting</rdfs:label>
    <rdfs:label xml:lang="nl">die tijdens dit bekend worden exploitant was</rdfs:label>
    <rdfs:label xml:lang="nl">te veranderen</rdfs:label>
