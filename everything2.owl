    <rdfs:label xml:lang="nl">bezitter</rdfs:label>
    <rdfs:label xml:lang="nl">uit zijn macht zijn gekomen</rdfs:label>
    <rdfs:label xml:lang="nl">vangen</rdfs:label>
  </rdf:Description>
