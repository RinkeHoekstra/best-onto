      a       skos:Concept .

onrechtmatige-daad:recht_op_een_goede_naam
      a       skos:Concept ;
      skos:broader onrechtmatige-daad:persoonlijkheidsrecht .

onrechtmatige-daad:recht_op_leven
      a       skos:Concept ;
      skos:broader onrechtmatige-daad:persoonlijkheidsrecht .

onrechtmatige-daad:recht_op_lichamelijke_integriteit
      a       skos:Concept ;
      skos:broader onrechtmatige-daad:persoonlijkheidsrecht .

onrechtmatige-daad:recht_op_privacy
      a       skos:Concept ;
      skos:broader onrechtmatige-daad:persoonlijkheidsrecht .

onrechtmatige-daad:recht_op_voortbrengselen_van_de_geest
