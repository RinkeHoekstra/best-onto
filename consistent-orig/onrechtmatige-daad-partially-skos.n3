                                    [ a       skos:Concept ;
                                      owl:unionOf ([ a       owl:Restriction ;
                                                  owl:onProperty onrechtmatige-daad:heeft_uitzondering ;
                                                  owl:someValuesFrom onrechtmatige-daad:uitzondering_bezit_6_173_174_179
                                                ] [ a       owl:Restriction ;
                                                  owl:onProperty onrechtmatige-daad:heeft_uitzondering ;
                                                  owl:someValuesFrom onrechtmatige-daad:uitzondering_ontbreken_aansprakelijkheid_6_162
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:heeft_vereiste ;
