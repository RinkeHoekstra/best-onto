                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:eigenschappen_van_het_product
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:garantie
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:leveringsvoowaarden
                          ] [ a       owl:Restriction ;
