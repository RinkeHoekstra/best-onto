                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:beschikbare_alternatieven
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:kenbaarheid
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:presentatie_van_het_product
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:ingevuld_door ;
                            owl:someValuesFrom onrechtmatige-daad:prijs_van_het_product
                          ] [ a       owl:Restriction ;
