                                                  owl:someValuesFrom onrechtmatige-daad:uitzondering_ontbreken_aansprakelijkheid_6_162
                                                ])
                                    ]
                          ] [ a       skos:Concept ;
                            owl:complementOf
                                    [ a       owl:Restriction ;
                                      owl:onProperty onrechtmatige-daad:heeft_vereiste ;
                                      owl:someValuesFrom onrechtmatige-daad:bekendheidseis
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:heeft_vereiste ;
                            owl:someValuesFrom onrechtmatige-daad:bezitter
                          ] [ a       owl:Restriction ;
                            owl:onProperty onrechtmatige-daad:heeft_vereiste ;
                            owl:someValuesFrom onrechtmatige-daad:eisen_die_men_daaraan_mag_stellen
                          ] [ a       owl:Restriction ;
