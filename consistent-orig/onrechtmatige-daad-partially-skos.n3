                                                [ a       owl:Restriction ;
                                                  owl:onProperty onrechtmatige-daad:ingevuld_door ;
                                                  owl:someValuesFrom onrechtmatige-daad:zeggenschap
                                                ]
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty onrechtmatige-daad:ingevuld_door ;
