<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:bestmap="http://www.best-project.nl/owl/bestmap.owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.best-project.nl/owl/bestmap.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <rdfs:seeAlso xml:lang="en">http://www.best-project.nl</rdfs:seeAlso>
    <owl:versionInfo xml:lang="en">This is version v0.1a</owl:versionInfo>
    <dc:creator xml:lang="en">Rinke Hoekstra (hoekstra@few.vu.nl)</dc:creator>
    <dc:rights xml:lang="en">Copyright (c) 2009, Rinke Hoekstra, Vrije Universiteit Amsterdam.

This ontology is made available under the Creative Commons Attribution 3.0 Netherlands Licence, see http://creativecommons.org/licenses/by/3.0/nl/</dc:rights>
    <rdfs:label xml:lang="en">BestMap Ontology</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:ID="Resource">
    <rdfs:comment xml:lang="en">A resource according to the BestMap ontology is any entity annotated using an 'about' relation with a skos:Concept</rdfs:comment>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="about"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Mapping"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broadMatch"/>
  <owl:ObjectProperty rdf:ID="d_about">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#about"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch"/>
  <owl:ObjectProperty rdf:ID="n_describes">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="nt_describes"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="cm_describes">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="describes"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#nt_describes">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#describes"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="em_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="cm_about"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrowMatch"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#relatedMatch"/>
  <owl:ObjectProperty rdf:ID="b_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broader"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="bt_about"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="nm_describes">
    <rdfs:subPropertyOf rdf:resource="#n_describes"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="em_describes">
    <rdfs:subPropertyOf rdf:resource="#cm_describes"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#cm_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#about"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="bm_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broadMatch"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#b_about"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="nt_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrowerTransitive"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#about"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
  <owl:ObjectProperty rdf:about="#describes">
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range rdf:resource="#Resource"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="rm_describes">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="r_describes"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="d_describes">
    <rdfs:subPropertyOf rdf:resource="#describes"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#related"/>
  <owl:ObjectProperty rdf:ID="n_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#nt_about"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="nm_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrowMatch"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#n_about"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#about">
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <owl:inverseOf rdf:resource="#describes"/>
    <rdfs:domain rdf:resource="#Resource"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#r_describes">
    <rdfs:subPropertyOf rdf:resource="#describes"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="r_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#related"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#about"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="rm_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#relatedMatch"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#r_about"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#bt_about">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="#d_about"/>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
    </owl:propertyChainAxiom>
    <rdfs:subPropertyOf rdf:resource="#about"/>
  </owl:ObjectProperty>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/rights"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
</rdf:RDF>

