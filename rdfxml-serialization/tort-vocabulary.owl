  <skos:Concept rdf:ID="eigenschappen_van_het_product">
    <skos:altLabel xml:lang="nl">hoeveelheid</skos:altLabel>
    <skos:altLabel xml:lang="nl">eigenschappen_van_het_product</skos:altLabel>
