  <best:Concept rdf:ID="onnodig_grievend">
    <skos:broader rdf:resource="#invulling_beoordelingscriterium"/>
    <skos:altLabel xml:lang="nl">mededeling</skos:altLabel>
    <skos:altLabel xml:lang="nl">waarde</skos:altLabel>
