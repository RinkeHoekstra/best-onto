tv:doen_of_nalaten_in_strijd_met_hetgeen_volgens_ongeschreven_recht_in_het_maatschappelijk_verkeer_betaamt
      mtv:same oldtv:in_strijd_met_hetgeen_in_het_maatschappelijk_verkeer_betaamt , oldtv:handelen_in_strijd_met_hetgeen_in_het_maatschappelijk_verkeer_betaamt , oldtv:maatschappelijke_betamelijkheid ;
      to:fingerprint
              [ to:value "redelijkheid elkaar verwachten"@nl ;
                to:weight 60;
