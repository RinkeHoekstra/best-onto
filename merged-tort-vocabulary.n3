tv:aansprakelijkheid
      mtv:same oldtv:aard_van_de_aansprakelijkheid , oldtv:aansprakelijkheid ;
      to:fingerprint
              [ to:value "aansprakelijkheid"@nl ;
                to:weight 100 ;
