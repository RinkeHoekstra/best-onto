        <rdfs:label xml:lang="nl"
            >degene die producten aanbiedt</rdfs:label>
        <rdfs:label xml:lang="nl">dienstverlener</rdfs:label>
        <rdfs:label xml:lang="nl"
            >in de uitoefening van beroep</rdfs:label>
        <rdfs:label xml:lang="nl"
            >in de uitoefening van beroep of bedrijf</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Een aanbieder is degene die goederen of diensten aanbiedt in de uitoefening van een beroep of bedrijf</rdfs:comment>
        <skos:prefLabel xml:lang="nl">aanbieder</skos:prefLabel>
        <skos:altLabel xml:lang="nl">dienstverlener</skos:altLabel>
        <skos:broader rdf:resource="#invulling_beoordelingscriterium"/>
