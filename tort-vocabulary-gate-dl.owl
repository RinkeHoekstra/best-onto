    <best:Concept rdf:about="#noodweerexces">
        <rdf:type rdf:resource="&skos;Concept"/>
        <rdfs:label xml:lang="nl">noodweer exces</rdfs:label>
        <rdfs:label xml:lang="nl">noodweerexces</rdfs:label>
        <rdfs:label xml:lang="nl"
            >schulduitsluitingsgrond</rdfs:label>
        <tort-ontology:fingerprint>
            <rdf:Description/>
        </tort-ontology:fingerprint>
    </best:Concept>
    


    <!-- http://www.best-project.nl/owl/tort-vocabulary.owl#norm_ter_bescherming_schade -->

    <best:Concept rdf:about="#norm_ter_bescherming_schade">
        <rdf:type rdf:resource="&skos;Concept"/>
        <rdfs:label xml:lang="nl"
            >norm die strekt ter bescherming van de schade</rdfs:label>
        <rdfs:label xml:lang="nl"
            >norm die strekt tot bescherming</rdfs:label>
        <rdfs:label xml:lang="nl"
