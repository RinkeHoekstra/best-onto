    <!-- http://www.best-project.nl/owl/tort-vocabulary.owl#onjuistheid_van_de_gegevens -->

    <best:Concept rdf:about="#onjuistheid_van_de_gegevens">
        <rdf:type rdf:resource="&skos;Concept"/>
        <rdfs:label xml:lang="nl">aannemer</rdfs:label>
        <rdfs:label xml:lang="nl">buizen</rdfs:label>
        <rdfs:label xml:lang="nl">gegevens</rdfs:label>
