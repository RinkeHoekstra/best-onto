    <!-- http://www.best-project.nl/owl/tort-vocabulary.owl#aard_van_de_gedraging -->

    <skos:Concept rdf:about="#aard_van_de_gedraging">
        <rdf:type rdf:resource="&best;Concept"/>
        <rdfs:label xml:lang="nl"
            >aard van de gedraging</rdfs:label>
        <rdfs:label xml:lang="nl"
