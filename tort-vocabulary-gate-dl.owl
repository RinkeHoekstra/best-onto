    <!-- http://www.best-project.nl/owl/tort-vocabulary.owl#aard_van_de_gepubliceerde_verdenking -->

    <skos:Concept rdf:about="#aard_van_de_gepubliceerde_verdenking">
        <rdf:type rdf:resource="&best;Concept"/>
        <rdfs:label xml:lang="nl"
            >aantasting van de eer</rdfs:label>
        <rdfs:label xml:lang="nl"
            >aantasting van de goede naam</rdfs:label>
        <rdfs:label xml:lang="nl"
            >aard van de gepubliceerde verdenking</rdfs:label>
        <rdfs:label xml:lang="nl">opmerking</rdfs:label>
